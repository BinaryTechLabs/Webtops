{
  "type": 1,
  "title": "WebTops",
  "name": "WebTops",
  "description": "Alpine, Ubuntu, Fedora, and Arch based containers containing full desktop environments in officially supported flavors accessible via any modern web browser.",
  "logo": "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/webtop-logo.png",
  "image": "lscr.io/linuxserver/webtop:latest",
  "note": "This is a note",
  "categories": [
    "Other",
    "Tools"
  ],
  "platform": "linux",
  "restart_policy": "unless-stopped",
  "ports": [
    "3001:3000/tcp"
  ],
  "network_mode": "bridge",
  "volumes": [
    {
      "container": "/config",
      "bind": "!config"
    }
  ],
  "sysctls": [
    {
    "net.ipv6.conf.all.disable_ipv6": "0"
    }
  ],
  "cap_add": [
    "NET_ADMIN"
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "1000"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "1000"
    },
    {
      "name": "KEYBOARD",
      "label": "KEYBOARD",
      "default": "en-us-qwerty"
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default": "America/Winnipeg"
    }
  ],
  "devices": [
    {
      "host": "/dev/dri",
      "container": "/dev/dri"
    }
  ]
}
